{
  "name": "weekend-projects",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && node scripts/prerender.js && cp static/404.html build/404.html",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
    "generate-routes": "node scripts/generate-routes.js",
    "prerender": "node scripts/prerender.js",
    "deploy": "npm run build && touch build/.nojekyll && gh-pages -d build"
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "^2.0.0",
    "@sveltejs/kit": "^1.20.4",
    "@tsconfig/svelte": "^5.0.4",
    "gh-pages": "^6.3.0",
    "mdsvex": "^0.12.3",
    "puppeteer": "^24.6.1",
    "svelte": "^4.0.5",
    "svelte-check": "^3.4.3",
    "svelte-preprocess": "^6.0.3",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.3",
    "typescript": "^5.0.0",
    "vite": "^4.4.2"
  },
  "type": "module",
  "dependencies": {
    "@emailjs/browser": "^4.4.1",
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-brands-svg-icons": "^6.7.2",
    "@fortawesome/free-regular-svg-icons": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/svelte-fontawesome": "^0.2.3",
    "@supabase/postgrest-js": "^1.8.5",
    "@supabase/supabase-js": "2.38.4",
    "@vapi-ai/web": "^2.2.6",
    "dotenv": "^16.4.7",
    "gray-matter": "^4.0.3",
    "marked": "^9.1.6",
    "uuid": "^11.1.0",
    "wavesurfer.js": "^7.9.4"
  }
}
